{
  "expo": {
    "name": "ERStudio App",
    "slug": "erstudio-staging-phase1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f3f3f3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSRemindersUsageDescription": "This app uses the CALENDAR to store appointment reminder."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#f3f3f3"
      },
      "package": "com.stevangd.erstudiostagingphase1",
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR",
        "REMINDERS",
        "CAMERA",
        "CAMERA_ROLL"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
